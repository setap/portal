<table class="table table-striped table-condensed">
    <thead>
    <tr>
        <th>#</th>
        <th>Дата создания</th>
        <th>Номер</th>
        <th>Описание</th>
        <th>Приоритет</th>
        <th>Доложил</th>
    </tr>
    </thead>
    <% for(var i in incidents) { %>
    <tr>
        <td><%= incidents[i].RN %></td>
        <td><%= incidents[i].CREATIONDATE %></td>
        <td><a href="#"><%= incidents[i].TICKETID %></a></td>
        <td><%= incidents[i].DESCRIPTION %></td>
        <td><%= incidents[i].INTERNALPRIORITY %></td>
        <td><%= incidents[i].REPORTEDBY %></td>
    </tr>
    <% } %>
</table>
